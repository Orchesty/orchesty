{
  "name": "hanaboso/pipes-framework",
  "description": "PipesFramework for Symfony",
  "version": "1.0.8",
  "autoload": {
    "psr-4": {
      "Hanaboso\\PipesFramework\\": "src/",
      "Tests\\": "tests/"
    }
  },
  "autoload-dev": {
    "psr-4": {
    }
  },
  "require": {
    "alcaeus/mongo-php-adapter": "^1.1",
    "bunny/bunny": "^0.2",
    "clue/buzz-react": "^2.1",
    "doctrine/doctrine-bundle": "^1.7",
    "doctrine/doctrine-fixtures-bundle": "^2.4",
    "doctrine/mongodb-odm-bundle": "^3.4",
    "doctrine/orm": "^2.5",
    "friendsofsymfony/rest-bundle": "^2.2",
    "guzzlehttp/guzzle": "^6.3",
    "jms-serializer/serializer-bundle": "^1.1",
    "league/oauth2-client": "^2.2",
    "nette/http": "^2.4",
    "nette/utils": "^2.4",
    "paragonie/halite": "^3.3",
    "phpoffice/phpspreadsheet": "@dev",
    "phpseclib/phpseclib": "^2.0",
    "ramsey/uuid": "^3.7",
    "symfony/assetic-bundle": "^2.8",
    "symfony/monolog-bundle": "^3.1",
    "symfony/swiftmailer-bundle": "^3.0",
    "symfony/symfony": "^3.3",
    "php-http/socket-client": "^1.0",
    "php-http/client-common": "^1.1",
    "php-http/message": "^1.0",
    "symfony/symfony": "^3.3",
    "snc/redis-bundle": "2.x-dev",
    "predis/predis": "^1.1"
  },
  "require-dev": {
    "pipes/php-check-utils": "^1.0",
    "sensio/framework-extra-bundle": "^3.0"
  },
  "repositories": [
    {
      "type": "path",
      "url": "../libs/php-check-utils",
      "options": {
        "symlink": false
      }
    }
  ]
}

{
    "name": "hanaboso/notification-sender",
    "description": "NotificationSender for Symfony",
    "version": "1.2.0",
    "license": "GPL-3.0-or-later",
    "require": {
        "php": "^7.4",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "ext-intl": "*",
        "ext-mbstring": "*",
        "hanaboso/commons-bundle": "^2.1",
        "hanaboso/email-service-bundle": "^1.1",
        "hanaboso/rabbit-mq-bundle": "^1.4",
        "hanaboso/rest-bundle": "^1.0",
        "hanaboso/utils": "^1.0"
    },
    "replace": {
        "ralouphie/getallheaders": "*",
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-intl-idn": "*",
        "symfony/polyfill-mbstring": "*",
        "symfony/polyfill-php72": "*",
        "symfony/polyfill-php73": "*"
    },
    "require-dev": {
        "hanaboso/php-check-utils": "^1.1"
    },
    "autoload": {
        "psr-4": {
            "Hanaboso\\NotificationSender\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "NotificationSenderTests\\": "tests"
        }
    }
}

{
  "name": "hanaboso/pipes-framework",
  "license": "proprietary",
  "description": "PipesFramework for Symfony",
  "version": "1.1.0",
  "autoload": {
    "psr-4": {
      "Hanaboso\\PipesFramework\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "require": {
    "php": "^7.3",
    "ext-json": "*",
    "ext-mongodb": "*",
    "ext-oauth": "*",
    "friendsofsymfony/rest-bundle": "^2.5",
    "hanaboso/acl-bundle": "^1.0",
    "hanaboso/email-service-bundle": "^1.0",
    "hanaboso/mongo-data-grid": "^1.1",
    "hanaboso/rabbit-mq-bundle": "^1.1",
      "hanaboso/pipes-php-sdk": "^1.0",
    "influxdb/influxdb-php": "^1.15",
    "league/oauth2-client": "^2.4",
    "mtdowling/cron-expression": "^1.2",
    "nette/utils": "^2.5",
    "php-http/client-common": "^1.9",
    "php-http/message": "^1.7",
    "php-http/socket-client": "^1.4",
    "phpoffice/phpspreadsheet": "^1.8",
    "snc/redis-bundle": "^2.1",
    "symfony/monolog-bundle": "^3.4",
    "symfony/symfony": "^4.3"
  },
  "require-dev": {
    "hanaboso/php-check-utils": "^1.0"
  },
  "repositories": [
    {
      "type": "path",
      "url": "../pipes-php-sdk",
      "options": {
        "symlink": true
      }
    },
    {
      "type": "git",
      "url": "git@gitlab.hanaboso.net:hanaboso/commons-bundle.git"
    },
    {
      "type": "git",
      "url": "git@gitlab.hanaboso.net:hanaboso/user-bundle.git"
    },
    {
      "type": "git",
      "url": "git@gitlab.hanaboso.net:hanaboso/email-service-bundle.git"
    },
    {
      "type": "git",
      "url": "git@gitlab.hanaboso.net:hanaboso/rabbit-mq-bundle.git"
    },
    {
      "type": "git",
      "url": "git@gitlab.hanaboso.net:hanaboso/acl-bundle.git"
    },
    {
      "type": "git",
      "url": "git@gitlab.hanaboso.net:hanaboso/MongoDataGrid.git"
    }
  ]
}

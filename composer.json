{
    "name": "hanaboso/pipes-framework",
    "description": "PipesFramework for Symfony",
    "license": "Apache-2.0",
    "version": "1.6.1",
    "require": {
        "php": "^8.0",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "ext-intl": "*",
        "ext-json": "*",
        "ext-mbstring": "*",
        "ext-mongodb": "*",
        "doctrine/orm": "^2.10",
        "hanaboso/acl-bundle": "^1.5",
        "hanaboso/app-store": "^1.6",
        "hanaboso/email-service-bundle": "^1.2",
        "hanaboso/mongo-data-grid": "^1.6",
        "hanaboso/pipes-php-sdk": "^1.6",
        "hanaboso/rest-bundle": "^1.1",
        "hanaboso/user-bundle": "^1.5",
        "influxdb/influxdb-php": "^1.15",
        "lcobucci/jwt": "^4.1",
        "ruflin/elastica": "^7.1",
        "symfony/security-bundle": "^v6.0",
        "symfony/serializer": "^v6.0"
    },
    "require-dev": {
        "hanaboso/php-check-utils": "^1.4",
        "predis/predis": "^v1.1"
    },
    "replace": {
        "ralouphie/getallheaders": "*",
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-intl-grapheme": "*",
        "symfony/polyfill-intl-icu": "*",
        "symfony/polyfill-intl-idn": "*",
        "symfony/polyfill-intl-normalizer": "*",
        "symfony/polyfill-mbstring": "*",
        "symfony/polyfill-php72": "*",
        "symfony/polyfill-php73": "*",
        "symfony/polyfill-php74": "*",
        "symfony/polyfill-php80": "*"
    },
    "repositories": [
        {
            "type": "path",
            "url": "../pipes-php-sdk",
            "options": {
                "symlink": true
            }
        },
        {
            "type": "path",
            "url": "../app-store",
            "options": {
                "symlink": true
            }
        }
    ],
    "autoload": {
        "psr-4": {
            "Hanaboso\\PipesFramework\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "PipesFrameworkTests\\": "tests",
            "UserBundleTests\\": "vendor/hanaboso/user-bundle/tests"
        }
    }
}

{
    "name": "orchesty/status-service",
    "license": "Apache-2.0",
    "version": "1.0.0",
    "require": {
        "php": "^8.1",
        "hanaboso/commons-bundle": "^2.6",
        "hanaboso/rabbit-mq-bundle": "^1.5",
        "hanaboso/utils": "^1.2",
        "symfony/console": "^v6.1",
        "symfony/framework-bundle": "^v6.1"
    },
    "require-dev": {
        "hanaboso/php-check-utils": "^1.4"
    },
    "replace": {
        "ralouphie/getallheaders": "*",
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-intl-grapheme": "*",
        "symfony/polyfill-intl-icu": "*",
        "symfony/polyfill-intl-idn": "*",
        "symfony/polyfill-intl-normalizer": "*",
        "symfony/polyfill-mbstring": "*",
        "symfony/polyfill-php72": "*",
        "symfony/polyfill-php73": "*",
        "symfony/polyfill-php74": "*",
        "symfony/polyfill-php80": "*",
        "symfony/polyfill-php81": "*"
    },
    "autoload": {
        "psr-4": {
            "Pipes\\StatusService\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "PipesStatusServiceTests\\": "tests/"
        }
    }
}

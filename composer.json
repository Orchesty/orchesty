{
    "name": "hanaboso/pipes-framework",
    "license": "proprietary",
    "description": "PipesFramework for Symfony",
    "version": "1.3.1",
    "autoload": {
        "psr-4": {
            "Hanaboso\\PipesFramework\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "PipesFrameworkTests\\": "tests"
        }
    },
    "require": {
        "php": "^7.4",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "ext-intl": "*",
        "ext-mbstring": "*",
        "doctrine/doctrine-fixtures-bundle": "^3.3",
        "hanaboso/acl-bundle": "^1.3",
        "hanaboso/app-store": "^1.3",
        "hanaboso/email-service-bundle": "^1.1",
        "hanaboso/mongo-data-grid": "~1.3.0",
        "hanaboso/pipes-php-sdk": "^1.2",
        "hanaboso/rabbit-mq-bundle": "^1.3",
        "hanaboso/rest-bundle": "^1.0",
        "hanaboso/user-bundle": "^1.3",
        "happyr/elastica-dsn": "^0.1",
        "influxdb/influxdb-php": "^1.15",
        "snc/redis-bundle": "^3.2",
        "symfony/serializer": "^5.0"
    },
    "require-dev": {
        "hanaboso/php-check-utils": "^1.1"
    },
    "replace": {
        "paragonie/random_compat": "*",
        "ralouphie/getallheaders": "*",
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-intl-idn": "*",
        "symfony/polyfill-mbstring": "*",
        "symfony/polyfill-php72": "*",
        "symfony/polyfill-php73": "*"
    },
    "repositories": [
        {
            "type": "path",
            "url": "../pipes-php-sdk",
            "options": {
                "symlink": true
            }
        },
        {
            "type": "path",
            "url": "../app-store",
            "options": {
                "symlink": true
            }
        }
    ]
}

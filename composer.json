{
    "name": "hanaboso/pipes-framework",
    "license": "proprietary",
    "description": "PipesFramework for Symfony",
    "version": "1.2.7",
    "autoload": {
        "psr-4": {
            "Hanaboso\\PipesFramework\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "require": {
        "php": "^7.4",
        "ext-json": "*",
        "ext-mongodb": "*",
        "ext-oauth": "*",
        "hanaboso/acl-bundle": "^1.3",
        "hanaboso/app-store": "^1.2",
        "hanaboso/email-service-bundle": "^1.0",
        "hanaboso/mongo-data-grid": "~1.3.0",
        "hanaboso/pipes-php-sdk": "^1.1",
        "hanaboso/rabbit-mq-bundle": "^1.3",
        "happyr/elastica-dsn": "^0.1",
        "influxdb/influxdb-php": "^1.15",
        "symfony/serializer": "^4.4 | ^5.0"
    },
    "require-dev": {
        "hanaboso/php-check-utils": "^1.1"
    },
    "repositories": [
        {
            "type": "path",
            "url": "../pipes-php-sdk",
            "options": {
                "symlink": true
            }
        },
        {
            "type": "path",
            "url": "../app-store",
            "options": {
                "symlink": true
            }
        }
    ]
}

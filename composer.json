{
    "name": "hanaboso/pipes-framework",
    "description": "PipesFramework for Symfony",
    "version": "1.5.0",
    "license": "GPL-3.0-or-later",
    "require": {
        "php": "^7.4",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "ext-intl": "*",
        "ext-json": "*",
        "ext-mbstring": "*",
        "ext-mongodb": "*",
        "hanaboso/acl-bundle": "^1.3",
        "hanaboso/app-store": "^1.5",
        "hanaboso/email-service-bundle": "^1.1",
        "hanaboso/mongo-data-grid": "~1.3.0",
        "hanaboso/pipes-php-sdk": "^1.5",
        "hanaboso/rabbit-mq-bundle": "^1.4",
        "hanaboso/rest-bundle": "^1.0",
        "hanaboso/user-bundle": "^1.3",
        "influxdb/influxdb-php": "^1.15",
        "predis/predis": "^1.1",
        "ruflin/elastica": "^7.0",
        "symfony/serializer": "^5.1"
    },
    "require-dev": {
        "hanaboso/php-check-utils": "^1.1"
    },
    "autoload": {
        "psr-4": {
            "Hanaboso\\PipesFramework\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "PipesFrameworkTests\\": "tests"
        }
    },
    "repositories": [
        {
            "type": "path",
            "url": "../pipes-php-sdk",
            "options": {
                "symlink": true
            }
        },
        {
            "type": "path",
            "url": "../app-store",
            "options": {
                "symlink": true
            }
        }
    ]
}

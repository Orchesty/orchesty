dbrp "pipes"."30m"

var sample = 4h
var retention = '4h'

batch
   |query('SELECT mean("avg.message") as "avg.message", max("max.message") as "max.message" FROM "pipes"."30m"."rabbitmq"')
       .period(sample)
       .every(sample)
       .groupBy('topology_id', 'node_id')
   |influxDBOut()
       .database('pipes')
       .measurement('rabbitmq')
       .precision('ms')
       .retentionPolicy(retention)
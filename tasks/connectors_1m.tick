dbrp "pipes"."5s"

var sample = 1m
var retention = '1m'

batch
   |query('SELECT mean("avg.time") as "avg.time",  min("min.time") as "min.time", max("max.time") as "max.time" FROM "pipes"."5s"."connectors"')
       .period(sample)
       .every(sample)
       .groupBy('topology_id', 'node_id')
   |influxDBOut()
       .database('pipes')
       .measurement('connectors')
       .precision('ms')
       .retentionPolicy(retention)
dbrp "pipes"."30m"

var sample = 4h
var retention = '4h'

batch
   |query('SELECT mean("avg.time") as "avg.time",  min("min.time") as "min.time", max("max.time") as "max.time" FROM "pipes"."30m"."connectors"')
       .period(sample)
       .every(sample)
       .groupBy('topology_id', 'node_id')
   |influxDBOut()
       .database('pipes')
       .measurement('connectors')
       .precision('ms')
       .retentionPolicy(retention)
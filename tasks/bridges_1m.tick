dbrp "pipes"."5s"

var sample = 1m
var retention = '1m'

batch
   |query('SELECT (sum("sum.time") / count("count.time")) as "avg_process.time", mean(avg_waiting.time) as "avg_waiting.time", sum(failed.count) as "failed.count", max(job_max.process) as "job_max.process", max(job_max.waiting) as "job_max.waiting", min(job_min.process) as "job_min.process", min(job_min.waiting) as "job_min.waiting", sum(total.count) as "total.count", sum("sum.time") as "sum.time", count("count.time") as "count.time" FROM "pipes"."5s"."bridges"')
       .period(sample)
       .every(sample)
       .groupBy('topology_id', 'node_id')
   |influxDBOut()
       .database('pipes')
       .measurement('bridges')
       .precision('ms')
       .retentionPolicy(retention)
dbrp "pipes"."1m"

var sample = 30m
var retention = '30m'

batch
   |query('SELECT (sum("sum.time") / sum("count.time")) as "avg.time",  min("min.time") as "min.time", max("max.time") as "max.time", sum("sum.time") as "sum.time", count("count.time") as "count.time" FROM "pipes"."1m"."connectors"')
       .period(sample)
       .every(sample)
       .groupBy('topology_id', 'node_id')
   |influxDBOut()
       .database('pipes')
       .measurement('connectors')
       .precision('ms')
       .retentionPolicy(retention)
dbrp "pipes"."1m"

var sample = 30m
var retention = '30m'

batch
   |query('SELECT (sum("sum.time") / count("count.time")) as "avg.message", max("max.message") as "max.message", sum("sum.time") as "sum.time", count("count.time") as "count.time" FROM "pipes"."1m"."rabbitmq"')
       .period(sample)
       .every(sample)
       .groupBy('topology_id', 'node_id')
   |influxDBOut()
       .database('pipes')
       .measurement('rabbitmq')
       .precision('ms')
       .retentionPolicy(retention)
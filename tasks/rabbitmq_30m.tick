dbrp "pipes"."1m"

var sample = 30m
var retention = '30m'

batch
   |query('SELECT mean("avg.message") as "avg.message", max("max.message") as "max.message" FROM "pipes"."1m"."rabbitmq"')
       .period(sample)
       .every(sample)
       .groupBy('topology_id', 'node_id')
   |influxDBOut()
       .database('pipes')
       .measurement('rabbitmq')
       .precision('ms')
       .retentionPolicy(retention)
{
  "name": "pipes",
  "version": "1.0.0",
  "description": "Hanaboso pipes and filters basic node skeleton implementation",
  "main": "dist/src/bin/pipes.js",
  "scripts": {
    "build": "rm -rf ./dist & ./node_modules/.bin/tsc",
    "lint": "./node_modules/.bin/tslint ./src/**/*.ts ./test/**/*.ts",
    "start": "node --use-strict ./dist/src/bin/pipes.js",
    "test": "NODE_ENV=test ./node_modules/mocha/bin/mocha dist/test/ --recursive --exit",
    "testunit": "NODE_ENV=test ./node_modules/mocha/bin/mocha dist/test/ --recursive --exit --grep '#unit' --retries 2",
    "testintegration": "NODE_ENV=test ./node_modules/mocha/bin/mocha dist/test/ --recursive --exit --grep '#integration' --timeout 10000  --retries 2",
    "fulltest": "npm run build && npm run lint && npm run test"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@bitbucket.org/HANABOSO/pipes.git"
  },
  "keywords": [
    "Pipes",
    "skeleton",
    "node"
  ],
  "author": "Hanaboso s.r.o.",
  "license": "ISC",
  "homepage": "https://bitbucket.org/HANABOSO/pipes#readme",
  "dependencies": {
    "amqplib": "^0.5.3",
    "amqplib-plus": "^0.0.18",
    "body-parser": "^1.18.3",
    "express": "^4.16.4",
    "hb-utils": "^0.0.4",
    "metrics-sender": "^0.0.8",
    "mongodb": "^3.2.3",
    "redis": "^2.8.0",
    "request": "^2.88.0",
    "request-promise": "^4.2.4",
    "uuid": "^3.3.2",
    "winston": "^3.2.1",
    "yargs": "^13.2.2"
  },
  "devDependencies": {
    "@types/amqplib": "^0.5.11",
    "@types/body-parser": "^1.17.0",
    "@types/chai": "^4.1.7",
    "@types/core-js": "^2.5.0",
    "@types/express": "^4.16.1",
    "@types/mocha": "^5.2.6",
    "@types/mongodb": "^3.1.23",
    "@types/node": "^10.14.4",
    "@types/power-assert": "^1.5.0",
    "@types/redis": "^2.8.12",
    "@types/request": "2.48.1",
    "@types/request-promise": "4.1.42",
    "@types/shuffle-array": "0.0.28",
    "@types/uuid": "^3.4.4",
    "@types/winston": "2.4.4",
    "@types/yargs": "^13.0.0",
    "chai": "^4.2.0",
    "chai-files": "^1.4.0",
    "chai-http": "^4.2.0",
    "chai-spies": "^1.0.0",
    "core-js": "^3.0.1",
    "mocha": "^6.1.3",
    "nock": "10.0.6",
    "shuffle-array": "^1.0.1",
    "prettier": "^1.17.0",
    "ts-mockito": "^2.3.1",
    "ts-node": "^8.1.0",
    "power-assert": "^1.6.1",
    "tslint": "^5.15.0",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^3.3.4000"
  }
}

{
  "name": "pipes",
  "version": "1.0.0",
  "description": "Hanaboso pipes and filters basic node skeleton implementation",
  "main": "dist/src/bin/pipes.js",
  "scripts": {
    "build": "rm -rf ./dist & ./node_modules/.bin/tsc",
    "lint": "./node_modules/.bin/tslint ./src/**/*.ts ./test/**/*.ts",
    "start": "node --use-strict ./dist/src/bin/pipes.js",
    "test": "NODE_ENV=test ./node_modules/mocha/bin/mocha dist/test/ --recursive --exit",
    "testunit": "NODE_ENV=test ./node_modules/mocha/bin/mocha dist/test/ --recursive --exit --grep '#unit' --retries 2",
    "testintegration": "NODE_ENV=test ./node_modules/mocha/bin/mocha dist/test/ --recursive --exit --grep '#integration' --timeout 10000  --retries 2",
    "fulltest": "npm run build && npm run lint && npm run test"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@bitbucket.org/HANABOSO/pipes.git"
  },
  "keywords": [
    "Pipes",
    "skeleton",
    "node"
  ],
  "author": "Hanaboso s.r.o.",
  "license": "ISC",
  "homepage": "https://bitbucket.org/HANABOSO/pipes#readme",
  "dependencies": {
    "amqplib": "^0.6.0",
    "amqplib-plus": "^1.0.3",
    "body-parser": "^1.19.0",
    "express": "^4.17.1",
    "hb-utils": "^0.0.4",
    "lodash": "^4.17.20",
    "metrics-sender": "^0.0.8",
    "moment": "^2.28.0",
    "mongodb": "^3.6.2",
    "redis": "^3.0.2",
    "request": "^2.88.2",
    "request-promise": "^4.2.6",
    "uuid": "^8.3.0",
    "winston": "^3.3.3",
    "yargs": "^16.0.3"
  },
  "devDependencies": {
    "@types/amqplib": "^0.5.13",
    "@types/body-parser": "^1.19.0",
    "@types/chai": "^4.2.12",
    "@types/core-js": "^2.5.4",
    "@types/express": "^4.17.8",
    "@types/mocha": "^8.0.3",
    "@types/mongodb": "^3.5.27",
    "@types/node": "^14.10.3",
    "@types/power-assert": "^1.5.3",
    "@types/redis": "^2.8.27",
    "@types/request": "^2.48.5",
    "@types/request-promise": "^4.1.46",
    "@types/shuffle-array": "^1.0.0",
    "@types/uuid": "^8.3.0",
    "@types/winston": "^2.4.4",
    "@types/yargs": "^15.0.5",
    "chai": "^4.2.0",
    "chai-files": "^1.4.0",
    "chai-http": "^4.3.0",
    "chai-spies": "^1.0.0",
    "core-js": "^3.6.5",
    "mocha": "^8.1.3",
    "nock": "13.0.4",
    "power-assert": "^1.6.1",
    "prettier": "^2.1.2",
    "shuffle-array": "^1.0.1",
    "ts-mockito": "^2.6.1",
    "ts-node": "^9.0.0",
    "tslint": "^6.1.3",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^4.0.2"
  }
}
